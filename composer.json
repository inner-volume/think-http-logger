{
  "name": "tp5er/think-http-logger",
  "description": "A Thinkphp package to log HTTP requests",
  "license": "MIT",
  "keywords": [
    "thinkphp",
    "think-http-logger"
  ],
  "authors": [
    {
      "name": "zhiqiang",
      "email": "zhiqiang2033@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2|^8.0.2",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "tp5er\\think\\HttpLogger\\": "src"
    }
  },
  "require-dev": {
    "topthink/framework": "^6.0",
    "tp5er/think-auth": "*"
  },
  "extra": {
    "think": {
      "services": [
        "tp5er\\think\\HttpLogger\\Service"
      ],
      "config": {
        "http-logger": "config/http-logger.php"
      }
    }
  }
}
